{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 450,
  "title": "Top 100 golf courses in Australia",
  "projection": {"type": "equalEarth"},
  "layer": [
    {
      "data": {
        "url":
        "https://github.com/BCohen7/assignment-two/blob/main/assignment-two/js/STE_2021_AUST_GDA2020.topojson",
        "format": {"type": "topojson", "feature": "STE_2021_AUST_GDA2020"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {  
    "data": {
      "url": "https://github.com/BCohen7/assignment-two/blob/main/assignment-two/data/courses.csv"},
    "mark": {"type": "circle", "opacity": 0.8, "size": 1},
    "encoding": {
      "longitude": {"field": "longitude", "type": "quantitative"},
      "latitude": {"field": "latitude", "type": "quantitative"}
    }
  }
  ]
}


{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "height": 400,
    "width": 400,
    "data": {"url": "https://raw.githubusercontent.com/BCohen7/assignment/main/assignment-two/data/courses.csv"},
    "transform": [{
        "calculate": "datum.Rank", "as": "rankPlot"
        }],
    "layer": [{
            "mark": {"type": "bar", "opacity": 0},
            "encoding": {
                "y": {"bin": true, "field": "binnedCount", "type": "ordinal"},
                "x": {"aggregate": "count", "scale": {"domain": [-10, 10]}}
            }
        },
        {
            "transform": [{
                "filter": "datum.PrivPub == 'Private'"
            }],
            "mark": {"type": "bar", "opacity": 1},
            "encoding": {
                "y": {"bin": true, "field": "rankPlot", "type": "ordinal"},
                "x": {"aggregate": "count", "scale": {"domain": [-10, 10]}},
                "tooltip": {"field": "Courses"}
            }
        }
    ],
    "config": {"background": "#ccecb5", "view": {"stroke": "transparent"}}
}